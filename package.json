{
  "name": "dependents-cli",
  "description": "CLI for listing an npm module's dependent projects and their download stats.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/dependents-cli",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/dependents-cli",
  "bugs": {
    "url": "https://github.com/doowb/dependents-cli/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "dependents": "bin/cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "co": "^4.6.0",
    "module-dependents": "^0.1.3",
    "yargs-parser": "^2.4.1"
  },
  "keywords": [
    "api",
    "cli",
    "dependents",
    "downloads",
    "module",
    "module-dependents",
    "npm",
    "npm-api",
    "npm-dependents",
    "package",
    "package-dependents"
  ],
  "verb": {
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "highlight": "module-dependents",
      "list": [
        "module-dependents",
        "npm-api",
        "npm-api-dependents",
        "download-stats"
      ]
    },
    "reflinks": [
      "verb-generate-readme",
      "verb"
    ]
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9"
  }
}
